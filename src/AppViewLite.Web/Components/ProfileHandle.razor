@code {
    [Parameter] public required BlueskyProfile Profile { get; set; }
}

<a data-handledid="@Profile.Did" class="handle-generic @(Profile.PossibleHandle == "handle.invalid" ? "handle-invalid" : Profile.HandleIsUncertain ? "handle-uncertain" : null)" href="@Profile.BaseUrl" tabindex="-1">@Profile.PossibleHandle</a>
